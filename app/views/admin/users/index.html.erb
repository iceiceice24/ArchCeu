<table class="table">
    <thead>
        <tr>
            <th scope="col">Email </th>
            <th scope="col">Role </th>
        </tr>
    </thead>

    <tbody>
        <% @user.each do |user | %>
        <tr>       
            <td> <%= user.email %> </td>
            <td> <%= user.role %> </td>
            <td> <%= link_to 'Edit User', edit_admin_user_path(user.id) %> </td>        
        </tr>
        <% end %>
    </tbody>
</table>