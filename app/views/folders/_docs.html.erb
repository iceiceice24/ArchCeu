<table>
  <thead>
    <tr>
      <th>FileName</th>
      <th>Created_at <%= link_to '▲', { sort: 'asc' } %><%= link_to '▼', { sort: 'desc' } %></th>
    </tr>
  </thead>

  <tbody>
    <% @folderFiles.each do |files|%>
    <tr>
      <td><%= link_to files.filename, rails_blob_path(files, disposition: "attachment") %></td>
      <td><%= files.created_at.strftime("%B %d, %Y") %></td>
    </tr>
    <% end %>
  </tbody>
</table>